server {
    listen 80;
    location /airbnb-onepage {
        proxy_pass http://127.0.0.1:5000;
    }

    location /airbnb-dynamic/ {
        rewrite ^/airbnb-dynamic/number_odd_or_even/(.*) /number_odd_or_even/$1;
    }
    location /number_odd_or_even/ {
        proxy_pass http://127.0.0.1:5001;
    }
}
